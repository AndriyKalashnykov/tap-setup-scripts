# AWS values, OK to omit if you are not installing on AWS
export AWS_REGION=us-east-2 # Pick a region.
export EKS_CLUSTER_NAME=my-aws-eks-cluster # Human-friendly name of EKS cluster, available in AWS Console.

# external image repo (e.g. DockerHub) values
export REGISTRY=user/build-service # DockerHub requires the format username/registry.  Other registries are different.
export REG_USERNAME=username
export REG_PASSWORD=password

# TAP values
export INFRASTRUCTURE_PROVIDER= # Leave blank or unassigned for local/kind install. Possible values: aws
export INSTALL_PROFILE=full # full or dev-light
export EXCLUDED_PACKAGES=basic # List of packages to not install. basic, testing, scanning or none.
export SUPPLY_CHAIN=testing # basic, testing, or scanning

export TN_USERNAME=user@example.com # Tanzu Network username
export TN_PASSWORD='password with a special character that requires single quotes !'

# The DOMAIN must be a static URL which does not change with the underlying IP or LoadBalancer.
# This means you cannot use clever DNS mapping sites such as `nip.io`
export DOMAIN=example.com
export SYS_DOMAIN="sys-subdomain.$DOMAIN" # if the tap-gui package is selected for installation and GUI_DOMAIN is not set, then SYS_DOMAIN must be set in order to avoid the prompt in the setup-tap.sh script.
export GUI_DOMAIN="ui-subdomain.$DOMAIN" # Only needed if the tap-gui package is selected for installation. NOTE: This is served on port 7000
export APPS_DOMAIN="apps-subdomain.$DOMAIN"
export EDUCATES_DOMAIN="learning-center-subdomain.$DOMAIN" # Only needed if learningcenter package is selected for installation

export GITHUB_ACCESS_TOKEN=your-github-access-token
export GUI_CATALOG_URL=https://github.com/your-registry/tap-blank-catalog/blob/main/catalog-info.yaml
